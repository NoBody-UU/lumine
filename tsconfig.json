{ 
  "compilerOptions": {
    "outDir": "./build",
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "strict": true,
    "skipLibCheck": true,

    "rootDir": "./src",
    "baseUrl": "./",
  },
  "exclude": ["node_modules"],
}