{ 
  "compilerOptions": {
    "outDir": "./build",
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "baseUrl": "./",
    "paths": {
      "#lumine/structures/*": ["./src/structures/*"],
      "#lumine/middlewares": ["./src/middlewares/index.ts"],
    }
  },
  "exclude": ["node_modules"],
}